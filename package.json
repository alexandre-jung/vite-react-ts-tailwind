{
  "name": "vite-react-ts",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://alexandre-jung.github.io/vite-react-ts-tailwind",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf ./coverage ./dist ./html-report ./storybook-static",
    "clean:all": "npm run clean -- ./node_modules",
    "coverage": "live-server --open=./coverage/lcov-report",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headless",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "format": "npm run sort-scripts && npm run prettier:fix && npm run lint:fix",
    "lint": "eslint \"{src,cypress}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier {src,cypress}/**/*.{js,jsx,ts,tsx,css,scss} ./*.{js,ts,html,json} --check",
    "prettier:fix": "npm run prettier -- --write",
    "preview": "vite preview",
    "report": "live-server --open=./html-report/report.html",
    "sort-scripts": "npx sort-package-json",
    "start": "vite",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:preview": "http-server ./storybook-static -p 8080",
    "test": "jest --reporters=default",
    "test:clear": "jest --clearCache",
    "test:coverage": "npm test -- --coverage",
    "test:report": "jest",
    "test:watch": "npm run test -- --watch",
    "update-screenshots": "cypress-image-diff -u"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.39",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "core-js": "^3.23.2",
    "cypress": "^10.3.0",
    "cypress-image-diff-js": "^1.19.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "7.30",
    "eslint-plugin-storybook": "^0.6.0",
    "gh-pages": "^4.0.0",
    "http-server": "^14.1.1",
    "immer": "^9.0.15",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-html-reporters": "^3.0.9",
    "live-server": "^1.2.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.capitalize": "^4.2.1",
    "msw": "^0.43.1",
    "node-html-parser": "^5.3.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "sass": "^1.52.3",
    "tailwindcss": "^3.1.4",
    "ts-jest": "^28.0.5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-svgr": "^2.1.1",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "src/public"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
