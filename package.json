{
  "name": "vite-react-ts",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://alexandre-jung.github.io/vite-react-ts-tailwind",
  "scripts": {
    "build": "tsc && vite build",
    "coverage": "live-server --open=./coverage/lcov-report",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headless",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "jest:clear": "jest --clearCache",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "preview": "vite preview",
    "report": "live-server --open=./html-report/report.html",
    "start": "vite",
    "test": "jest --reporters=default",
    "test:report": "jest",
    "test:watch": "npm run test -- --watch",
    "update-screenshots": "cypress-image-diff -u",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:preview": "npx http-server ./storybook-static",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.13",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "camelcase": "^6.3.0",
    "core-js": "^3.22.4",
    "cypress": "^10.1.0",
    "cypress-image-diff-js": "^1.19.2",
    "eslint": "^8.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "7.30",
    "eslint-plugin-storybook": "^0.5.12",
    "gh-pages": "^4.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-html-reporters": "^3.0.8",
    "live-server": "^1.2.2",
    "msw": "^0.42.1",
    "postcss": "^8.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "vite": "^2.9.8",
    "vite-plugin-svgr": "^2.1.0",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "src/public"
  }
}
